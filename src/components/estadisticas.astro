<style>

    .datosEmpresa{
        width: 100%;
        height: 90px;
        -background-color: black;
        display: flex;
        flex-direction: row;
        justify-content: space-between;
        gap:1rem;
    }

    .contdatos{
        width: 100%;
        height: 100%;
        -background-color: yellow;
        display: flex;
        flex-direction: row;
        align-items: center;
        gap: 1em;
        flex: calc(33.3% - 2rem);
    }

    .datosIcon{
        width: 100%;
        height: 100%;
        -background-color:yellow;
    }

    .numDatos{
        width: 100%;
        height: 100%;
        -background-color: rgba(255, 255, 255, 0.7);
    }

    .numeroText{
        font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        font-size: 3em;
        font-weight: 500;
        letter-spacing: -0.01em;
        margin: 0;
        padding: 0;
        line-height:initial;
        color: var(--color-base);
    }

    .bajadaText{
        text-wrap: wrap;
        font-size: 0.7em;
        margin: 0;
        padding: 0;
        line-height:initial;
        color: rgb(68, 68, 78);
        margin-top: -5px;
    }

    .datosIcon{
        position: relative;
        width:100%;
        height: 100%;
        display: flex;
        flex-direction: column;
		justify-content: center;
		align-items: end;
        -background-color: green;
    }

    .circuloIcon{
        width: 85px;
        height: 85px;
        border-radius: 100px;
        display: flex;
        flex-direction: column;
		justify-content: center;
		align-items: center;
        background-color: var(--color-base);
    }

    .iconosNumeros{
        width: 60px; 
        height: 60px;
        margin-left: 12px;
    }

    .iconosNumerosDos{
        width: 60px; 
        height: 60px;
        margin-left: 2px;
    }

    @media (min-width: 768px) and (max-width: 1024px) {
        .contdatos{
            gap: 0em;
        }

        
        .datosIcon{
            position: relative;
            width:100%;
            height: 100%;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            -background-color: green;
        }

        .numeroText{
            font-size: 2.6em;
            font-weight: bold;
        }   
        
        .bajadaText{
            font-size: 0.5em;
            margin-top: -5px;
            font-weight: bold; 
        }

    }

    @media (min-width: 320px) and (max-width: 767px) {

        main{
            padding: 0em;
        }

        .datosEmpresa{
            height: fit-content;
            gap: 0.5rem;
        }

        .contdatos{
            gap: 0.5rem;
            flex-direction: column;
            justify-content: center;
            align-items: center;
        }

        .datosIcon{
            align-items: center;
        }

        .circuloIcon{
            width: 80px;
            height: 80px;
        }

        .iconosNumeros{
            width: 50px;
            height: 50px;
        }

        .iconosNumerosDos{
            width: 55px;
            height: 55px;
        }

        .numeroText{
            font-size: 2.3em;
            font-weight: bold;
            text-align: center;
        }   
        
        .bajadaText{
            font-size: 0.75em;
            margin-top: -1px;
            font-weight: normal; 
            text-align: center;
            line-height: 0.9em;
        }
    }
    

</style>